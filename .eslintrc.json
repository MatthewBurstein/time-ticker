{
    "extends": "airbnb-base",
    "env": {
      "jest": true,
      "jquery": true,
      "browser": true
    },
    "rules": {
      "semi": ["error", "never"],
      "no-underscore-dangle": [
        "error",
        {
          "allowAfterThis": true
        }
      ],
      "no-unused-vars": [
        "error",
        { "varsIgnorePattern": "_" }
      ]
    }
}